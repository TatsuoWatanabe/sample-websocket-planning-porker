{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "attach",
			"name": "Attach to Process",
			"processId": "${command:PickProcess}",
			"port": 5858
		},
		{
			"name": "Launch",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/app/bin/www.js",
			"stopOnEntry": false,
			"args": [],
			"cwd": "${workspaceRoot}/",
			"runtimeExecutable": null,
			"runtimeArgs": [
				"--nolazy",
        "--harmony"
			],
			"env": {
				"NODE_ENV": "development",
        "PORT": "5000",
				"MONGODB_URI": "mongodb://127.0.0.1/man-hours-manager-db",
				"SUPER_SECRET": "secret",
				"ALLOW_ORIGIN": "http://localhost:3000"
			},
			"console": "externalTerminal",
			"sourceMaps": true,
			"outFiles": ["${workspaceRoot}/app"]
		},
		{
			"name": "Attach",
			"type": "node",
			"request": "attach",
			"port": 5858
		}
	]
}